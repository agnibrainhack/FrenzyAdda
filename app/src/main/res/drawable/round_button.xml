<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#c6b04e" />
            <gradient android:angle="-90" android:startColor="#6e1d16" android:endColor="#df6046"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#b8c774" />
            <solid android:color="#cebf60"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#dc6b52" />
            <gradient android:angle="-90" android:startColor="#ea4537" android:endColor="#d92b1b" />
        </shape>
    </item>
</selector>